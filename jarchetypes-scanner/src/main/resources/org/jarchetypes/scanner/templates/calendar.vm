<table style="margin-bottom: 10px; margin: 0">
	<tr> 
		<td><h:outputLabel for="${widget.getFieldName()}" value="${widget.getTitle()}" /> 
		</td> 
		<td>
				
			<p:calendar id="${widget.getFieldName()}" value="#{${CRUDBean}.${widget.getBeanName()}.${widget.getFieldName()}}"		  	 
		  	 #if(${widget.getAttribute("mode")}!="") mode="${widget.getAttribute("mode")}" #end	
		  	 #if(${widget.getAttribute("maxdate")}!="") maxdate="${widget.getAttribute("maxdate")}" #end
		  	 #if(${widget.getAttribute("mindate")}!="") mindate="${widget.getAttribute("mindate")}" #end
		  	 #if(${widget.getAttribute("navigator")}!="") navigator="${widget.getAttribute("navigator")}" #end
		  	 #if(${widget.getAttribute("pattern")}!="") pattern="${widget.getAttribute("pattern")}" #end
		  	 #if(${widget.getAttribute("showOn")}!="") showOn="${widget.getAttribute("showOn")}" #end/>
			
		</td>
	</tr>
</table>